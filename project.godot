; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="spacecraft3d"
config/version="0.0.1"
run/main_scene="res://ui/menus/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.png"

[autoload]

Utils="*res://utils/utils_autoload.gd"
BlockLibrary="*res://block_library/block_library_autoload.tscn"
ObjectSelection="*res://ui/object_selection/object_selection_autoload.gd"
FpsCounterAutoload="*res://ui/fps_counter/fps_counter_autoload.tscn"
SceneStack="*res://addons/scene_stack/autoload.gd"
ExtraDebugShapes="*res://addons/extra_debug_shapes/autoload.gd"
JoypadInputEcho="*res://addons/joypad_input_echo/autoload.gd"
ScreenshotCapture="*res://addons/screenshot_capture/autoload.gd"
GameOptions="*res://addons/game_options/autoload.gd"
InputMonitor="*res://addons/input_icons/autoload.gd"
SceneTextures="*res://addons/scene_texture/autoload.gd"
EmulateMouseFromTouch="*res://addons/emulate_mouse_from_touch/autoload.gd"
BuildingMechanics="*res://addons/building_mechanics/autoload.gd"
BadDataMonitor="*res://addons/bad_data_monitor/autoload.gd"

[display]

window/size/viewport_width=1600
window/size/viewport_height=900

[editor]

movie_writer/movie_file="export/movie.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/bad_data_monitor/plugin.cfg", "res://addons/building_mechanics/plugin.cfg", "res://addons/emulate_mouse_from_touch/plugin.cfg", "res://addons/extra_debug_shapes/plugin.cfg", "res://addons/game_options/plugin.cfg", "res://addons/input_icons/plugin.cfg", "res://addons/joypad_input_echo/plugin.cfg", "res://addons/physics_interpolation/plugin.cfg", "res://addons/player_controller/plugin.cfg", "res://addons/scene_stack/plugin.cfg", "res://addons/scene_texture/plugin.cfg", "res://addons/screenshot_capture/plugin.cfg")

[file_customization]

folder_colors={
"res://game/": "red",
"res://ui/build_menu/": "green",
"res://ui/input_hints/": "green",
"res://ui/menus/": "green"
}

[game_options]

game_options_scene="res://game_options.tscn"

[gui]

theme/custom="res://theme.tres"

